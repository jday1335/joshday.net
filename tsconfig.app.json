{
  "compilerOptions": {
    "composite": true,
    "target": "ES2020",
    "lib": [ "ES2020", "DOM", "DOM.Iterable" ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "types": [ "vite/client" ]
  },
  "include": [ "src", "vite-env.d.ts" ]
}